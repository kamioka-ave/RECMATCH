<section class="content-header">
  <h1>居住地国・外国PEPs・FATCA届出の履歴</h1>
</section>

<% if @histories.size > 0 %>
  <%= react_component('views.admin.students.peps.histories.IndexView',
      {student: @student, pep: @pep, history: @histories.last, min: @histories.first.revision, max: @histories.last.revision}) %>
<% else %>
  <section class="content">
    履歴はありません
  </section>
<% end %>
